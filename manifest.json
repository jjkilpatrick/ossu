{
    "name": "ossu",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Ossu - Notification Centre",
    "icons": {
        "16": "webapp/library/images/ossu-16.png",
        "128": "webapp/library/images/ossu-128.png"
    },
    "chrome_url_overrides": {
        "newtab": "webapp/index.html"
    },
    "permissions": [
        "storage",
        "geolocation",
        "tabs",
        "http://api.wunderground.com/api",
        "*://use.typekit.net/*",
        "webRequest",
        "webRequestBlocking"
    ],
    "content_security_policy": "script-src 'self' https://use.typekit.net; object-src 'self'"
}